import std;

fn main() {
    loop i int = 1; i <= 100; i++ {
        if i % 15 == 0 {
            std.println("FizzBuzz");
        } else if i % 3 == 0 {
            std.println("Fizz");
        } else if i % 5 == 0 {
            std.println("Buzz");
        } else {
            std.println(i);
        }
    }
}