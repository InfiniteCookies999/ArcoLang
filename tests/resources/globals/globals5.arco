// -- Test to make sure that default
//    initialization of arrays of struct
//    objects works globally.
import std;

Data struct {
    a int = 235;
    b int = 31;
}

arr Data[5];

fn main() {
    loop i int = 0; i < 5; i++ {
        std.print(arr[i].a);
        std.print(" ", 1);
        std.print(arr[i].b);
        std.print(" ", 1);
    }
}