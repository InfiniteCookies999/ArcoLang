import std;

Day enum {
    MONDAY     :: 6;
    TUESDAY    :: 9;
    WEDNESDAY  :: 6;
    THURSDAY   :: 1;
    FRIDAY     :: 7;
    SATURDAY   :: 2;
    SUNDAY     :: 1;
}

fn main() {
    std.print(Day.MONDAY);
    std.print(" ", 1);
    std.print(Day.TUESDAY);
    std.print(" ", 1);
    std.print(Day.WEDNESDAY);
    std.print(" ", 1);
    std.print(Day.THURSDAY);
    std.print(" ", 1);
    std.print(Day.FRIDAY);
    std.print(" ", 1);
    std.print(Day.SATURDAY);
    std.print(" ", 1);
    std.print(Day.SUNDAY);
}