import std;

Day enum {
    MONDAY     :: 2;
    TUESDAY    :: 3;
    WEDNESDAY  :: 4;
    THURSDAY   :: 5;
    FRIDAY     :: 6;
    SATURDAY   :: 7;
    SUNDAY     :: 8;
}

fn main() {
    std.print(Day.MONDAY);
    std.print(" ", 1);
    std.print(Day.TUESDAY);
    std.print(" ", 1);
    std.print(Day.WEDNESDAY);
    std.print(" ", 1);
    std.print(Day.THURSDAY);
    std.print(" ", 1);
    std.print(Day.FRIDAY);
    std.print(" ", 1);
    std.print(Day.SATURDAY);
    std.print(" ", 1);
    std.print(Day.SUNDAY);
}