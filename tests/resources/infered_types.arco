import std;

A struct {
    f1 int;
    f2 int;
}

CONSTANT :: 6;

fn main() {
    a := 5;
    b := "a message!";
    c := [ 16, 31 ];
    d := A{ 53, 99 };
    e, f := 8, "nice";
    
    std.print(a);
    std.print(" ", 1);
    std.print(b, 10);
    std.print(" ", 1);
    std.print(c[0]);
    std.print(" ", 1);
    std.print(c[1]);
    std.print(" ", 1);
    std.print(d.f1);
    std.print(" ", 1);
    std.print(d.f2);
    std.print(" ", 1);
    std.print(CONSTANT);
    std.print(" ", 1);
    std.print(e);
    std.print(" ", 1);
    std.print(f, 4);
}