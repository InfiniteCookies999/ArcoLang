
Message enum {
    FIRST  :: "Programming is fun";
    SECOND :: "cool message";
}

generics<T>
fn foo(message T, length int) {
    the_message Message = message;
    the_message_value cstr = the_message;
    print(the_message_value, length);
}

fn main() {
    foo(Message.FIRST, 18);
    print(" ", 1);
    foo(Message.SECOND, 12);
}