import std;
import std.reflect.TypeId;

generics\T\
my_constraint2 :: typeid(T) == TypeId.INT;

generics\T\
my_constraint :: my_constraint2\T\;

generics\my_constraint T\
fn foo(a T) {
    std.print("constrait foo");
}

fn foo(a cstr) {
    std.print("no constraint foo");
}

fn main() {
    foo(325);
    std.print(" ");
    foo("abcdef");
}