import std;

fn main() {
    a int = 0;
    a++;
    ++a;
    std.print(a);
    std.print(" ", 1);
    a--;
    --a;
    std.print(a);
    std.print(" ", 1);
    std.print(a++);
    std.print(" ", 1);
    std.print(a);
    std.print(" ", 1);
    std.print(a--);
    std.print(" ", 1);
    std.print(a);
    std.print(" ", 1);
    std.print(++a);
    std.print(" ", 1);
    std.print(--a);
}