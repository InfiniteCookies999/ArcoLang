// -- Test to make sure that 'break' and 'continue'
//    statements work.
import std;

fn main() {
    loop i int = 0; i < 100; i = i + 1 {
        if i > 50 {
            break;
        }
        if i % 2 == 0 {
            continue;
        }
        std.print(i);
        std.print(" ", 1);
    }
}