// -- Test to make sure that varargs
//    functions can work with other non
//    varargs parameters.
import std;

fn foo(first int, args int...) {
    std.print(first);
    std.print(" ", 1);
    std.print(args[0]);
    std.print(" ", 1);
    std.print(args[1]);
}

fn main() {
    foo(23, 62, 11);
}