    if true && true {
        std.print("case1", 5);
    }
    if true && false {
        std.print("case2", 5);
    }
    if false && true {
        std.print("case3", 5);
    }
    if false && false {
        std.print("case4", 5);
    }
    b1 bool = true;
    b2 bool = true;
    if b1 && b2 {
