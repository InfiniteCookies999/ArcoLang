
fn strlen(s cstr) int {
	length int = 0;
	loop ptr cstr = s; *ptr != '\0'; ++ptr {
		++length;
	}
	return length;
}

fn print(msg cstr, length int) {
	handle        void* = GetStdHandle(-11);
	bytes_written uint32;
	WriteFile(handle, msg, length, &bytes_written, null);
}

fn print(msg cstr) {
	print(msg, strlen(msg));
}

fn println(msg cstr) {
	print(msg, strlen(msg));
	print("\n", 1);
}